#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules/ts-node/dist/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules/ts-node/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules/ts-node/dist/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules/ts-node/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.16_@types+node@20.17.30_typescript@5.8.2/node_modules:/Users/emutis/Documents/ts/speech-to-text/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-script.js" "$@"
else
  exec node  "$basedir/../../dist/bin-script.js" "$@"
fi
